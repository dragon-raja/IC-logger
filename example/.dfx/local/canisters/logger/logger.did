type View = 
 record {
   messages: vec text;
   start_index: nat;
 };
type TextLogger = 
 service {
   allow: (vec principal) -> () oneway;
   append: (vec text) -> () oneway;
   stats: () -> (Stats) query;
   view: (nat, nat) -> (View) query;
 };
type Stats = 
 record {
   bucket_sizes: vec nat;
   start_index: nat;
 };
service : () -> TextLogger
